{
  "name": "sfdx-codescan-plugin",
  "description": "Run CodeScan or SonarQube jobs from sfdx",
  "version": "1.0.9",
  "author": "Ben van Klinken @ustramooner, Barys Yakavita @borisbsu",
  "bugs": "https://github.com/codescan-io/sfdx-codescan-plugin/issues",
  "dependencies": {
    "@oclif/core": "^3.19.1",
    "@salesforce/command": "^5",
    "@salesforce/core": "^6.5.3",
    "copy-dir": "^1.3.0",
    "expand-home-dir": "0.0.3",
    "find-java-home": "^0.2.0",
    "node-unzip-2": "^0.2.8",
    "tslib": "^2",
    "axios": "^1.6.7" 
  },
  "devDependencies": {
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^2",
    "@salesforce/ts-sinon": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-header": "^3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-typescript": "^0",
    "globby": "^14.0.1",
    "mocha": "^9",
    "nyc": "^15",
    "oclif": "^3",
    "prettier": "^2",
    "rimraf": "latest",
    "sinon": "10.0.0",
    "ts-node": "^10",
    "typescript": "^4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/lib",
    "/assets",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/codescan-io/sfdx-codescan-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to run codescan."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "VillageChief/sfdx-codescan-plugin",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "rimraf lib && tsc -b && oclif manifest && oclif readme",
    "test": "nyc --extension .ts --require ts-node/register mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  }
}
